module CPUArray
in  inary@1024 with default(0) : Int      # 入力ノードでも配列は使用可能
out oary@1024 : Int      # 出力ノードでも配列を使用可能
use

node init[false] hoge@2048 with default(0) : Bool = if 11 == inary[self/2] then true else false

node oary@1024 with default(0) : Int = if oary@last[self] != 0 then inary[self*2] else hoge[self]
